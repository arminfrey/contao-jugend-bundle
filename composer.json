{
    "name": "arminfrey/contao-jugend-bundle",
    "description": "Erfassung der Jugendlichen in einem eigenen Modul, damit nicht jeder Zugriff hat.",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4",
        "tag 5",
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Armin Frey",
            "email": "webmaster@krettenweiber.de",
            "homepage": "https://github.com/arminfrey",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/arminfrey/contao-jugend-bundle/issues",
        "source": "https://github.com/arminfrey/contao-jugend-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.9  || ^5.0"
    },
   
    "autoload": {
        "psr-4": {
            "Arminfrey\\ContaoJugendBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Arminfrey\\ContaoJugendBundle\\ContaoManager\\Plugin"
    }
}
